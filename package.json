    "scripts": {
        "ng": "ng",
        "dev": "ng serve --port 4201",
        "test-lib": "ng test natural",
        "test-demo": "ng test demo",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build natural && cp -v README.md dist/natural/ && scss-bundle -e ./projects/natural/src/lib/_natural.theme.scss -d ./dist/natural/theming/_natural.theme.scss && cd dist/natural && yarn version --no-git-tag-version --new-version `git describe --tags --always`",
        "build-demo": "yarn build-lib && ng build demo --prod --aot --base-href /natural/ && cp dist/demo/index.html dist/demo/404.html",
        "llink": "cd dist/natural && (yarn unlink ; yarn link)"
    },
    "private": true,
