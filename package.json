{
    "name": "demo",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve",
        "test-lib": "ng test natural",
        "test-demo": "ng test demo",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build natural && cp -v README.md dist/natural/ && scss-bundle --config scss-bundle.config.json && cd dist/natural && sed -i'' -e '/prepublishOnly/d' package.json && yarn version --no-git-tag-version --new-version `git describe --tags --always`",
        "build-demo": "yarn build-lib && ng build demo --prod --aot --base-href /natural/ && cp dist/demo/index.html dist/demo/404.html",
        "llink": "cd dist/natural && (yarn unlink ; yarn link)"
    },
    "sideEffects": false,
    "private": true,
    "dependencies": {
        "@angular/animations": "^10.1.2",
