{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "demo": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "i18n": {
                "sourceLocale": "fr",
                "locales": {
                    "ar": "projects/natural/i18n/messages.ar.xlf",
                    "br": "projects/natural/i18n/messages.br.xlf",
                    "cs": "projects/natural/i18n/messages.cs.xlf",
                    "de": "projects/natural/i18n/messages.de.xlf",
                    "en": "projects/natural/i18n/messages.en.xlf",
                    "es": "projects/natural/i18n/messages.es.xlf",
                    "is": "projects/natural/i18n/messages.is.xlf",
                    "it": "projects/natural/i18n/messages.it.xlf",
                    "ku": "projects/natural/i18n/messages.ku.xlf",
                    "lb": "projects/natural/i18n/messages.lb.xlf",
                    "pl": "projects/natural/i18n/messages.pl.xlf",
                    "pt": "projects/natural/i18n/messages.pt.xlf",
                    "zh": "projects/natural/i18n/messages.zh.xlf"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/demo",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": ["src/styles.scss"],
                        "scripts": [],
                        "allowedCommonJsDependencies": ["graphql-tag", "zen-observable"]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "i18nMissingTranslation": "ignore",
                            "localize": true
                        },
                        "fr": {
                            "localize": ["fr"]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "port": 4201,
                        "browserTarget": "demo:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "demo:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "demo:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": ["src/styles.scss"],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["tsconfig.app.json", "tsconfig.spec.json", "e2e/tsconfig.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "demo:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "demo:serve:production"
                        }
                    }
                }
            }
        },
        "natural": {
            "projectType": "library",
            "root": "projects/natural",
            "sourceRoot": "projects/natural/src",
            "prefix": "natural",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/natural/tsconfig.lib.json",
                        "project": "projects/natural/ng-package.json"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/natural/src/test.ts",
                        "tsConfig": "projects/natural/tsconfig.spec.json",
                        "karmaConfig": "projects/natural/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/natural/tsconfig.lib.json", "projects/natural/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                },
                "xliffmerge": {
                    "builder": "@ngx-i18nsupport/tooling:xliffmerge",
                    "options": {
                        "xliffmergeOptions": {
                            "i18nFormat": "xlf",
                            "srcDir": "dist/",
                            "genDir": "projects/natural/i18n",
                            "defaultLanguage": "fr",
                            "languages": [
                                "fr",
                                "ar",
                                "br",
                                "cs",
                                "de",
                                "en",
                                "es",
                                "is",
                                "it",
                                "ku",
                                "lb",
                                "pl",
                                "pt",
                                "zh"
                            ]
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "demo"
}
